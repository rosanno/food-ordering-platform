generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model restaurant {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  menuName     String
  price        String
  discount     String?
  imageUrl     String?
  metaTitle    String
  metaKeywords String

  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
